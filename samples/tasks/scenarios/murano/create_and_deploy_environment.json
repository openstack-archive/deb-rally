{
    "MuranoEnvironments.create_and_deploy_environment": [
        {
            "args": {
                "packages_per_env": 2
            },
            "runner": {
                "type": "constant",
                "times": 2,
                "concurrency": 2
            },
            "context": {
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 2
                },
                "murano_packages": {
                    "app_package": "rally-jobs/extra/murano/applications/HelloReporter/io.murano.apps.HelloReporter.zip"
                },
                "roles": ["admin"]
            }
        },
        {
            "args": {
                "packages_per_env": 2
            },
            "runner": {
                "type": "constant",
                "times": 2,
                "concurrency": 2
            },
            "context": {
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 2
                },
                "murano_packages": {
                    "app_package": "rally-jobs/extra/murano/applications/HelloReporter/io.murano.apps.HelloReporter/"
                },
                "roles": ["admin"]
            }
        }
    ]
}
