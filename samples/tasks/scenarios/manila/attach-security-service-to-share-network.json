{
    "ManilaShares.attach_security_service_to_share_network": [
        {
            "args": {
                "security_service_type": "active_directory"
            },
            "runner": {
                "type": "constant",
                "times": 10,
                "concurrency": 10
            },
            "context": {
                "users": {
                    "tenants": 1,
                    "users_per_tenant": 1
                },
                "quotas": {
                    "manila": {
                        "share_networks": -1
                    }
                }
            }
        }
    ]
}
